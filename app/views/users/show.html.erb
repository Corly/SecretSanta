<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Auth token:</strong>
  <%= @user.auth_token %>
</p>

<p>
  <strong>Uid:</strong>
  <%= @user.uid %>
</p>

<p>
	<strong>Events</strong>
	<%= @events.each do |event| %>
		<li><%= event.id %></li>
	<% end %>
</p>

<p>
	<strong>Friends</strong>
	<%= @friends.each do |friend| %>
		<li><%= friend %></li>
	<% end %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
